package main;
import java.util.*;
public class Main {
	public static void main(String[] args) {
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		
		int n;
		
		System.out.print("Quantas pessoas serao digitadas? ");
		n = sc.nextInt();
		
		for(int i = 0; i < n; i++) {
			System.out.println("Dados da " + i + 1 + "ª pessoa :");
		}
		
		
		sc.close();
	}
}