package main;
import java.util.*;
public class Main {
	public static void main(String[] args) {
		Locale.setDefault(Locale.US);
		
		Scanner sc = new Scanner(System.in);
		
		int minutos;
		double valorPagar;
		
		System.out.print("Digite a quantidade de minutos: ");
		minutos = sc.nextInt();
		
		System.out.println("" + String.format("%.2f", valorPagar));
		sc.close();
	}
}